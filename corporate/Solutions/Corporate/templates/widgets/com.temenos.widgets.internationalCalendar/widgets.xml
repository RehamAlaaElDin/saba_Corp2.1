<?xml version="1.0" encoding="UTF-8"?><widgets>
  <widget displayName="International Calendar" icon="images/internationalCalendar_logo.jpg" name="InternationalCalendar" type="Date">
    <template type="source" value="internationalCalendar_body.wgt"/>
    <template type="header" value="internationalCalendar_head.wgt"/>
    <custom>
      <item defaultValue="persian" help="By calendar type is understood the calendar class. I.e.: persian(Iranian), gregorian" includeInDesign="Always" layout="-1" mand="true" name="CalendarType" prompt="Calendar Type" tab="Calendar Settings" type="com.acquire.intelligentforms.ide.dialogs.RichTextField|RICH_VALUE" usage="7"/>
      <item includeInDesign="Always" help="The language of the calendar. I.e.: en, fa, cs, da, de, es, fi, fr, hu, nl, pl, pt, ro, sk, zh" layout="-1" mand="false" name="Locale" prompt="Locale" tab="Calendar Settings" type="com.acquire.intelligentforms.ide.dialogs.RichTextField|RICH_VALUE" usage="7"/>
      <item defaultValue="dd/MM/YYYY" help="Calendar date format" includeInDesign="Always" layout="-1" mand="true" name="CalendarDateFormat" prompt="Calendar Date Format" tab="Calendar Settings" type="com.acquire.intelligentforms.ide.dialogs.RichTextField|RICH_VALUE" usage="7"/>
      <item defaultValue="hh:mm" help="Calendar time format" includeInDesign="Always" layout="-1" mand="true" name="CalendarTimeFormat" prompt="Calendar Time Format" tab="Calendar Settings" type="com.acquire.intelligentforms.ide.dialogs.RichTextField|RICH_VALUE" usage="7"/>
      <item includeInDesign="Always" help="The days of the year that will be made read-only, written in this format dd/mm/yyyy, separated by a comma. I.e.: 01/05/2017,25/12/2017 etc." layout="-1" mand="false" name="LegalHolidays" prompt="Legal Holidays (i.e. dd/mm/yyyy)" tab="Calendar Settings" type="com.acquire.intelligentforms.ide.dialogs.RichTextField|RICH_VALUE" usage="7"/>
      <item includeInDesign="Always" help="Mark legal holidays as read-only" layout="-1" mand="false" name="MarkLegalHolidaysAsReadOnly" prompt="Mark Legal Holidays As Read-Only" tab="Calendar Settings" type="com.acquire.util.FocusCheckbox" usage="7"/>
      <item includeInDesign="Always" help="The weekend days that will be made read-only separated by a comma. I.e.:0,6 (this means Sunday and Saturday)" layout="-1" mand="false" name="WeekendDays" prompt="Weekend Days (i.e. 0-sunday, 1-monday etc.)" tab="Calendar Settings" type="com.acquire.intelligentforms.ide.dialogs.RichTextField|RICH_VALUE" usage="7"/>
      <item includeInDesign="Always" help="Mark weekend days names as read-only" layout="-1" mand="false" name="MarkWeekendDaysAsReadOnly" prompt="Mark Weekend Days Names As Read-Only" tab="Calendar Settings" type="com.acquire.util.FocusCheckbox" usage="7"/>
      <item includeInDesign="Always" help="Icon for the date chooser" layout="-1" mand="false" name="CalendarImage" prompt="Calendar Image" tab="Calendar Appearance" type="javax.swing.JFileChooser|MULTI_RICH_VALUES" typeValue="Select a file|FILES_ONLY|jpg,gif,png,tiff,bmp|Image files|Open|Project dir" usage="7"/>
      <item includeInDesign="Always" help="The colour of the date bar" layout="-1" mand="false" name="DateBarColour" prompt="Date Bar Colour" tab="Calendar Appearance" type="com.acquire.util.ui.StyleSheetColorChooser" typeValue="standard" usage="7"/>
      <item includeInDesign="Always" help="The background colour for the valid dates" layout="-1" mand="false" name="ValidDateColour" prompt="Valid Date Colour" tab="Calendar Appearance" type="com.acquire.util.ui.StyleSheetColorChooser" typeValue="standard" usage="7"/>
      <item includeInDesign="Always" help="The background colour for the invalid dates" layout="-1" mand="false" name="InvalidDateColour" prompt="Invalid Date Colour " tab="Calendar Appearance" type="com.acquire.util.ui.StyleSheetColorChooser" typeValue="standard" usage="7"/>
      <item includeInDesign="Always" help="The backround colour for the selected date" layout="-1" mand="false" name="SelectedDateColour" prompt="Selected Date Colour" tab="Calendar Appearance" type="com.acquire.util.ui.StyleSheetColorChooser" typeValue="standard" usage="7"/>
	  <item includeInDesign="Always" help="The background colour for the holidays" layout="-1" mand="false" name="HolidayColor" prompt="Holiday Colour" tab="Calendar Appearance" type="com.acquire.util.ui.StyleSheetColorChooser" typeValue="standard" usage="7"/>
	  <item includeInDesign="Always" help="The background colours for the weekend" layout="-1" mand="false" name="WeekendColor" prompt="Weekend Colour" tab="Calendar Appearance" type="com.acquire.util.ui.StyleSheetColorChooser" typeValue="standard" usage="7"/>
      <item includeInDesign="Always" help="Comma separated list of the long week names" layout="-1" mand="false" name="LongWeekNames" prompt="Weekdays Names (i.e. Saturday,Sunday etc.)" tab="Calendar Settings" type="com.acquire.intelligentforms.ide.dialogs.RichTextField|RICH_VALUE" usage="15"/>
      <item includeInDesign="Always" help="Comma separated list of the short week names" layout="-1" mand="false" name="ShortWeekNames" prompt="Short Weekdays Names (i.e. Sa,Su etc.)" tab="Calendar Settings" type="com.acquire.intelligentforms.ide.dialogs.RichTextField|RICH_VALUE" usage="15"/>
      <item includeInDesign="Always" help="Comma separated list of the month names" layout="-1" mand="false" name="LongMonthNames" prompt="Month Names (i.e. January,February etc.)" tab="Calendar Settings" type="com.acquire.intelligentforms.ide.dialogs.RichTextField|RICH_VALUE" usage="15"/>
      <item includeInDesign="Always" help="Comma separated list of the short week names" layout="-1" mand="false" name="ShortMonthNames" prompt="Short Month Names (i.e. Jan,Feb etc.)" tab="Calendar Settings" type="com.acquire.intelligentforms.ide.dialogs.RichTextField|RICH_VALUE" usage="15"/>
    </custom>
    <template type="header" value="internationalCalendar_languages.wgt"/>
	<template type="header" value="internationalCalendar_noconflict.wgt"/>
  </widget>
</widgets>
